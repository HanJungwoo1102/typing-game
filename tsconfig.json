{
  "compilerOptions": {
    "outDir": "./public/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports" : true,
    "esModuleInterop" : true,
    "baseUrl": ".",
    "paths": {
      "page/*": ["src/page/*"],
      "router/*": ["src/lib/router/*"]
    }
  }
}